{
  "labels": ["dependencies"],
  "separateMajorMinor": true,
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch"
    }
  ],
  "lockFileMaintenance": {
    "enabled": true
  }
}
